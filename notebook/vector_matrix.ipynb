    "'''\n",
    "矩阵与二维数组，下标返回值的区别。\n",
    "'''\n",
    "m1 = np.array ( [\n",
    "    [ 1, 2, 3, 4],\n",
    "    [ 5, 6, 7, 8]\n",
    "] )\n",
    "\n",
    "m2 = np.matrix ( [\n",
    "    [ 1, 2, 3, 4],\n",
    "    [ 5, 6, 7, 8]\n",
    "] )\n",
    "\n",
    "print ( m1 [0] )    #返回的不在是二维，而是一维数组\n",
    "print ( m2 [0] )   #返回的还是二维数组\n"
   ]
